<h1>Listing foundries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Location</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @foundries.each do |foundry| %>
  <tr>
    <td><%= foundry.name %></td>
    <td><%= foundry.description %></td>
    <td><%= foundry.location %></td>
    <td><%= foundry.url %></td>
    <td><%= link_to 'Show', foundry %></td>
    <td><%= link_to 'Edit', edit_foundry_path(foundry) %></td>
    <td><%= link_to 'Destroy', foundry, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Foundry', new_foundry_path %>
